        "#Exercise1: Sum up integer powers\n",
        "def power_sumint(N,i):\n",
        "  sum=0\n",
        "  for n in range(1,N+1):\n",
        "    sum+=(n**i)\n",
        "  return sum\n",
        "\n",
        "i=int(input(\"enter your i value\"))\n",
        "N=int(input(\"enter your N value\"))\n",
        "sum=power_sumint(N,i)\n",
        "print(\"the required sum is\",sum)\n"
      ]
